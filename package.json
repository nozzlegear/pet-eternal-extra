{
    "name": "pet-eternal-extra",
    "version": "1.5.5",
    "description": "Extra scripts and utils used by https://www.peteternal.com",
    "main": "dist/index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start:dev": "yarn validate && parcel serve --no-cache -d parcel-serve-dist --port 3000 index.html",
        "build": "yarn validate && parcel build --no-cache -t browser index.tsx",
        "validate": "tsc -p . --skipLibCheck --noEmit",
        "prepublishOnly": "yarn build"
    },
    "author": "Joshua Harms <joshua@nozzlegear.com>",
    "license": "MIT",
    "browserslist": [
        "last 2 Chrome versions",
        "last 2 Firefox versions",
        "last 2 Safari versions",
        "last 2 Edge versions"
    ],
    "devDependencies": {
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.4",
        "@types/reqwest": "github:nozzlegear/reqwest.d.ts#4287b39",
        "parcel": "^1.12.3",
        "stylus": "^0.54.5",
        "typescript": "^3.5.3"
    },
    "dependencies": {
        "image-blobber": "^1.2.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "regenerator-runtime": "^0.13.2",
        "reqwest": "^2.0.5"
    }
}
